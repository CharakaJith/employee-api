    "env-cmd": "^10.1.0",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.4",
    "nanoid": "^5.0.7",
    "randomstring": "^1.3.0",
    "sequelize": "^6.37.2",
